---
import Layout from "../layouts/layout.astro";
import CinemaComponent from "../cinema";

const siteTitle = "cinema"
const description = "i love it. if you love it we should talk. link : https://cal.com/shivamhwp/cinema"
---
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" href="/favicon.svg" />
    <meta name="description" content={description} />
    <meta property="twitter:title" content="shivam">
    <meta name="twitter:image" content="https://utfs.io/f/38t7p527clgqX5dOcQ7v8sPQUuYndb0xI2wjoA1efSJzOtcV" />
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:creator" content="@shivamhwp" />
    <meta property="og:title" content={siteTitle} />
    <meta property="og:description" content={description} />
    <meta property="og:site_name" content="shivam.ing">
    <meta property="og:url" content="shivam.ing">
    <meta property="og:image" content="https://utfs.io/f/38t7p527clgqX5dOcQ7v8sPQUuYndb0xI2wjoA1efSJzOtcV" />
    <title>{siteTitle}</title>
</head>

<Layout>
    <CinemaComponent client:load />
</Layout>